<%
	String id = request.getParameter("id");
	String treat = null;
	
	if(id.equals("admin")){
		treat ="정성을 다해 모심";
	}else {
		treat ="내쫓음";
	}
%>

<h1>Data Extraction</h1>
<hr>
<h2>id : <%= id %></h2>
<h2><%=treat%></h2>
<h2>name : <%=request.getParameter("name")%></h2>
<button><%= id +"" + treat + "" + request.getParameter("name") %></button>
</body>
